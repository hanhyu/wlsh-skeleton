{
  "name": "wlsh/core",
  "description": "运行环境：php8.1 / swoole5.1",
  "keywords": [
    "swoole",
    "php"
  ],
  "authors": [
    {
      "name": "hanhyu",
      "email": "hanhyu@qq.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.3",
    "ext-swoole": ">=5.1.2",
    "ext-json": "*",
    "ext-redis": ">=5.3.2",
    "monolog/monolog": "2.2.0",
    "symfony/mailer": "6.0.3",
    "swiftmailer/swiftmailer": "^6.3"
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://mirrors.aliyun.com/composer/"
    }
  },
  "autoload": {
    "psr-4": {
      "Wlsh\\": "./src",
      "Modules\\": "tests/example/App/Modules",
      "Domain\\": "tests/example/App/Domain",
      "Models\\": "tests/example/App/Models"
    }
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "friendsofphp/php-cs-fixer": "^3.40.0",
    "phpunit/phpunit": "^9.5.0",
    "swoole/ide-helper": "5.1.2"
  },
  "suggest": {
    "ext-mongodb": ">=1.9.0",
    "ext-redis": ">=5.3.2",
    "monolog/monolog": "2.2.0",
    "symfony/mailer": "6.0.3"
  }
}
